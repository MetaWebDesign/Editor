            priority="normal" 
            contentProvider="modelo.diagram.navigator.ModeloDomainNavigatorContentProvider" 
            labelProvider="modelo.diagram.navigator.ModeloDomainNavigatorLabelProvider"
            icon="icons/obj16/ModeloDiagramFile.gif"
            activeByDefault="true">
         <triggerPoints>
