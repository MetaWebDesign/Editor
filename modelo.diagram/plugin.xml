            contentProvider="modelo.diagram.navigator.ModeloNavigatorContentProvider" 
            labelProvider="modelo.diagram.navigator.ModeloNavigatorLabelProvider"
            icon="icons/obj16/ModeloDiagramFile.gif"
            activeByDefault="true">
         <triggerPoints>
            <or>
	           <and>
    	          <instanceof value="org.eclipse.core.resources.IFile"/>
        	      <test property="org.eclipse.core.resources.extension" value="modelo_diagram"/>
               </and>
               <instanceof value="modelo.diagram.navigator.ModeloAbstractNavigatorItem"/>
            </or>
         </triggerPoints>
         <possibleChildren>
            <or>
         	   <instanceof value="modelo.diagram.navigator.ModeloAbstractNavigatorItem"/>
            </or>
         </possibleChildren>
         <commonSorter 
               id="modelo.diagram.navigatorSorter" 
               class="modelo.diagram.navigator.ModeloNavigatorSorter">
            <parentExpression>
               <or>
	              <and>
    	             <instanceof value="org.eclipse.core.resources.IFile"/>
        	         <test property="org.eclipse.core.resources.extension" value="modelo_diagram"/>
                  </and>
                  <instanceof value="modelo.diagram.navigator.ModeloAbstractNavigatorItem"/>
               </or>
            </parentExpression>
         </commonSorter>
      </navigatorContent>
      <navigatorContent
